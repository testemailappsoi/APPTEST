{% extends 'base.html.twig' %}

{% block title %}Application{% endblock %}

{% block body %}
<div class="row">
    <div class="col-6 mx-auto">

    <h1>Application</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ application.id }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ application.Version }}</td>
            </tr>
            <tr>
                <th>NomApp</th>
                <td>{{ application.NomApp }}</td>
            </tr>
        </tbody>
    </table>

    <div class="modal-footer">
            <a href="{{ path('app_application_index') }}" class="btn btn-secondary">Retour</a>
            <a class="btn btn-theme" href="{{ path('app_application_edit', {'id': application.id}) }}">Modifier</a>
 {{ include('application/_delete_form.html.twig') }}
  </div>

    
     </div>
</div>  
{% endblock %}
